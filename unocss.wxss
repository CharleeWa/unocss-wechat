/* layer: default */
.m-20 {
  margin: 160rpx;
}

.mb-10 {
  margin-bottom: 80rpx;
}

.mb-5 {
  margin-bottom: 40rpx;
}

.h-128 {
  height: 1024rpx;
}

.w-128 {
  width: 1024rpx;
}

.flex {
  display: flex;
}

.flex-col {
  flex-direction: column;
}

@keyframes swing {
  20% {
    transform: rotate3d(0, 0, 1, 15deg)
  }

  40% {
    transform: rotate3d(0, 0, 1, -10deg)
  }

  60% {
    transform: rotate3d(0, 0, 1, 5deg)
  }

  80% {
    transform: rotate3d(0, 0, 1, -5deg)
  }

  to {
    transform: rotate3d(0, 0, 1, 0deg)
  }
}

.animate-swing {
  animation: swing 1s linear 1;
  transform-origin: top center;
}

.animate-iteration-infinite {
  animation-iteration-count: infinite;
}

.items-center {
  align-items: center;
}

.overflow-hidden {
  overflow: hidden;
}

.rounded-lg {
  border-radius: 16rpx;
}

.border-none {
  border-style: none;
}

.bg-green-500 {
  --un-bg-opacity: 1;
  background-color: rgba(34, 197, 94, var(--un-bg-opacity));
}

.bg-green-700 {
  --un-bg-opacity: 1;
  background-color: rgba(21, 128, 61, var(--un-bg-opacity));
}

.bg-hex-81ecec_50 {
  background-color: #81ecec_50;
}

.px-4 {
  padding-left: 32rpx;
  padding-right: 32rpx;
}

.py-2 {
  padding-top: 16rpx;
  padding-bottom: 16rpx;
}

.text-center {
  text-align: center;
}

.font-semibold {
  font-weight: 600;
}

.text-hex-8e44ad {
  --un-text-opacity: 1;
  color: rgba(142, 68, 173, var(--un-text-opacity));
}

.text-white {
  --un-text-opacity: 1;
  color: rgba(255, 255, 255, var(--un-text-opacity));
}

.shadow-md {
  --un-shadow: var(--un-shadow-inset) 0 4px 6px -1px var(--un-shadow-color, rgba(0, 0, 0, 0.1)), var(--un-shadow-inset) 0 2px 4px -2px var(--un-shadow-color, rgba(0, 0, 0, 0.1));
  box-shadow: var(--un-ring-offset-shadow), var(--un-ring-shadow), var(--un-shadow);
}
